<?xml version='1.0' encoding='UTF-8'?>
<shortcuts>
	<shortcut>
		<label>Kodi Games</label>
		<label2>Games</label2>
		<icon />
		<action>ActivateWindow(Games,return)</action>
	</shortcut>
	<shortcut>
		<label>Advanced Emulator Launcher</label>
		<label2>Games</label2>
		<icon />
		<action>ActivateWindow(Programs,plugin://plugin.program.advanced.emulator.launcher,return)</action>
		<visible>System.HasAddon(plugin.program.advanced.emulator.launcher)</visible>
	</shortcut>
	<shortcut>
		<label>AEL Favorites</label>
		<label2>Favorites</label2>
		<icon />
		<action>ActivateWindow(Programs,"plugin://plugin.program.advanced.emulator.launcher/?com=SHOW_FAVOURITES",return)</action>
		<visible>System.HasAddon(plugin.program.advanced.emulator.launcher)</visible>
	</shortcut>
